@model List<CollageViewModel>


<table class="table">
    <thead>
        <tr>
            <th>Collage id</th>
            <th>Collage Name</th>
            <th>Collage Deception</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.NameEnglish</td>
                <td>@item.DescriptionEnglish</td>
            </tr>
        }
    </tbody>
</table>